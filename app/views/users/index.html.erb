<div class="col-md-12">
  <h2>User List</h2>
</div>
<div class="row justify-content-center">
  <table class="table">
    <tr>
      <th>Name</th>
      <th>Email</th>
    </tr>
    <% @users.each do |user| %>
      <tr>
        <td><%= link_to user.name, user %></td>
        <td><%= user.email %></td>
      </tr>
    <% end %>
  </table>
</div>
